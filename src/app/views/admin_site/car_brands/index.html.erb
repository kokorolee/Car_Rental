<p id="notice"><%= notice %></p>

<h1>Car Brands</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @car_brands.each do |car_brand| %>
      <tr>
        <td><%= car_brand.code %></td>
        <td><%= car_brand.name %></td>
        <td><%= link_to 'Show', @car_brand %></td>
        <td><%= link_to 'Edit', edit_admin_site_car_brand_path(car_brand) %></td>
        <td><%= link_to 'Destroy', admin_site_car_brand_path(car_brand), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Car Brand', new_admin_site_car_brand_path %>
